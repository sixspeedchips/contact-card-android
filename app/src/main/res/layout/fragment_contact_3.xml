<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  xmlns:tools="http://schemas.android.com/tools"
  android:id="@+id/nav_contact"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context=".controller.ContactFragment"
  >


  <EditText
    android:id="@+id/parsed_name"
    style="@style/editText"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:hint="@string/name"
    app:layout_constraintBottom_toTopOf="@+id/parsed_phone"
    app:layout_constraintTop_toBottomOf="@+id/raw_text"
    />

  <EditText
    android:id="@+id/parsed_phone"
    style="@style/editText"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="8dp"
    android:hint="@string/phone"
    app:layout_constraintBottom_toTopOf="@id/parsed_email"
    app:layout_constraintTop_toBottomOf="@id/parsed_name"
    />

  <EditText
    android:id="@+id/parsed_email"
    style="@style/editText"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:hint="@string/email"
    app:layout_constraintBottom_toBottomOf="parent"
    app:layout_constraintTop_toBottomOf="@+id/parsed_phone"
    />

  <TextView
    android:id="@+id/raw_text"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:gravity="center"
    app:layout_constraintBottom_toTopOf="@id/parsed_name"
    app:layout_constraintTop_toTopOf="parent"
    />


</androidx.constraintlayout.widget.ConstraintLayout>
