<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
  xmlns:tools="http://schemas.android.com/tools"
  xmlns:app="http://schemas.android.com/apk/res-auto"
  android:id="@+id/contact"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  tools:context=".controller.ContactFragment"
  android:background="@android:color/white"
  >

  <EditText
    android:id="@+id/parsed_name"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:layout_constraintBottom_toTopOf="@+id/parsed_phone"
    app:layout_constraintTop_toBottomOf="@+id/raw_text"
    />

  <EditText
    android:id="@+id/parsed_phone"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="8dp"
    app:layout_constraintBottom_toTopOf="@id/parsed_email"
    app:layout_constraintTop_toBottomOf="@id/parsed_name"
    />

  <EditText
    android:id="@+id/parsed_email"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:layout_constraintBottom_toBottomOf="@id/parsed_name"
    app:layout_constraintTop_toBottomOf="@+id/parsed_phone"
    />

  <TextView
    android:id="@+id/raw_text"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="108dp"

    app:layout_constraintTop_toTopOf="parent"
    tools:layout_editor_absoluteX="0dp"/>

</androidx.constraintlayout.widget.ConstraintLayout>